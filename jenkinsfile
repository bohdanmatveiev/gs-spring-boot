pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Build') {
            steps {
                sh 'cd complete && mvn clean package'
            }
        }

        stage('Test') {
            steps {
                sh 'cd complete && mvn test'
            }
        }

        stage('Archive artifacts') {
            steps {
                archiveArtifacts artifacts: 'complete/target/*.jar', fingerprint: true
            }
        }
    }
}
